class Warehouse {
  constructor(apiKey) {
    this.apiKey = apiKey;
  }

  items(){
    let items
    $.ajax({
      url: "http://localhost:3000/api/inventory?api_key=" + this.apiKey,
      dataType: 'json',
      method: 'GET',
      success: function(data) {
        items = data;
      },
      async: false
    }).fail(function($xhr) {
      let data = $xhr.responseJSON;
      console.log(data);
    });
    return items
  }

  skuItem(sku){
    let item
    $.ajax({
      url: "http://localhost:3000/api/inventory/" + sku + "?api_key=" + this.apiKey,
      dataType: 'json',
      method: 'GET',
      success: function(data) {
        item = data;
      },
      async: false
    }).fail(function($xhr) {
      let data = $xhr.responseJSON;
      console.log(data);
    });
    return item
  }
}


